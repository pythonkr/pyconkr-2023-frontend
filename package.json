    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.0",
    "react-markdown": "^8.0.5",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.0",
    "react-table": "^7.8.0",
    "recoil": "^0.7.6",
